               "       FD  OUTPUT-FILE                  .                      "
                                        
               "           MOVE SOURCE-FACSIMILE (20) TO.                      "
                                        
               "                                        .                      "
                                        
               " OUTPUT-RECORD.                         .                      "
                                        
               "           RECORDING MODE F             .                      "
                                        
               "           WRITE OUTPUT-RECORD.         .                      "
                                        
               "                                        .                      "
                                        
               "                                        .                      "
                                        
