               "                                        .                      "
                                        
               "B-X) TO FILLER-DATA.                    .                      "
                                        
               "       01  OUTPUT-RECORD                .                      "
                                        
               "           MOVE FILLER-IMAGE TO OUTPUT-R.                      "
                                        
               "     PIC X(80).                         .                      "
                                        
               "ECORD.                                  .                      "
                                        
               "       WORKING-STORAGE SECTION.         .                      "
                                        
               "           WRITE OUTPUT-RECORD.         .                      "
                                        
               "                                        .                      "
                                        
               "                                        .                      "
                                        
               "       01  SUB-X                        .                      "
                                        
               "           MOVE SOURCE-FACSIMILE (20) TO.                      "
                                        
