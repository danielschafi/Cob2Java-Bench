               " OUTPUT-RECORD.                         .                      "
                                        
               "       01  SOURCE-FACSIMILE-AREA.       .                      "
                                        
               "           WRITE OUTPUT-RECORD.         .                      "
                                        
               "                                        .                      "
                                        
               "                                        .                      "
                                        
               "           02  SOURCE-FACSIMILE-DATA.   .                      "
                                        
               "           MOVE SOURCE-FACSIMILE-TWO (SU.                      "
                                        
               "                                        .                      "
                                        
               "B-X) TO FILLER-DATA.                    .                      "
                                        
               "               03  FILLER               .                      "
                                        
               "           MOVE FILLER-IMAGE TO OUTPUT-R.                      "
                                        
               "     PIC X(40) VALUE                    .                      "
                                        
               "ECORD.                                  .                      "
                                        
               "           02  SOURCE-FACSIMILE-TABLE RE.                      "
                                        
               "           WRITE OUTPUT-RECORD.         .                      "
                                        
               "DEFINES                                 .                      "
                                        
               "                                        .                      "
                                        
               "                   SOURCE-FACSIMILE-DATA.                      "
                                        
               "           IF  SUB-X < 68               .                      "
                                        
               ".                                       .                      "
                                        
               "                                        .                      "
                                        
               "               03  SOURCE-FACSIMILE OCCU.                      "
                                        
               "               ADD 1 TO SUB-X           .                      "
                                        
               "RS 68.                                  .                      "
                                        
               "                                        .                      "
                                        
               "                   04  SOURCE-FACSIMILE-.                      "
                                        
               "               GO TO ML-3.              .                      "
                                        
               "ONE  PIC X(40).                         .                      "
                                        
               "                                        .                      "
                                        
               "                   04  SOURCE-FACSIMILE-.                      "
                                        
               "           MOVE 21 TO SUB-X.            .                      "
                                        
               "TWO  PIC X(40).                         .                      "
                                        
               "                                        .                      "
                                        
